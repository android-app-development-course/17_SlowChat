<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.chat.entity.UserTagEntity" table="user_tag" schema="chat">
        <id name="id">
            <column name="id" sql-type="int(10) unsigned"/>
        </id>
        <property name="userId">
            <column name="user_id" sql-type="smallint(5) unsigned"/>
        </property>
        <property name="tagId">
            <column name="tag_id" sql-type="smallint(5) unsigned"/>
        </property>
        <many-to-one name="userByUserId" class="com.chat.entity.UserEntity">
            <column name="user_id"/>
        </many-to-one>
        <many-to-one name="tagByTagId" class="com.chat.entity.TagEntity">
            <column name="tag_id"/>
        </many-to-one>
    </class>
</hibernate-mapping>