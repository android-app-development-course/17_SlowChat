<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.chat.entity.TagEntity" table="tag" schema="chat">
        <id name="id">
            <column name="id" sql-type="smallint(5) unsigned"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(60)" length="60"/>
        </property>
        <set name="userTagsById" inverse="true">
            <key>
                <column name="tag_id"/>
            </key>
            <one-to-many not-found="ignore" class="com.chat.entity.UserTagEntity"/>
        </set>
    </class>
</hibernate-mapping>